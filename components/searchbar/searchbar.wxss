/* component/searchbar/searchbar.wxss */

.searchbar {
  background: #41be57;
  padding: 20rpx;
}

.search-navigator {
  width: 100%;
  height: 60rpx;
  background-color: #fff;
  border-radius: 10rpx;
  background-image: url("data:image/png;base64,");
  background-position: center center;
  background-repeat: no-repeat;
  background-size: 5%;
}

.search-inp-group{
  width: 100%;
  height: 60rpx;
  background-color: #fff;
  border-radius: 10rpx;
  padding:10rpx;
  box-sizing: border-box;
}

.search-inp{
  min-height: 40rpx;
  height: 40rpx;
  font-size: 12px;
}

